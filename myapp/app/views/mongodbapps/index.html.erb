<h1>Listing mongodbapps</h1>

<table>
  <thead>
    <tr>
      <th>People</th>
      <th>Keyw</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mongodbapps.each do |mongodbapp| %>
      <tr>
        <td><%= mongodbapp.people %></td>
        <td><%= mongodbapp.keyw %></td>
        <td><%= link_to 'Show', mongodbapp %></td>
        <td><%= link_to 'Edit', edit_mongodbapp_path(mongodbapp) %></td>
        <td><%= link_to 'Destroy', mongodbapp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mongodbapp', new_mongodbapp_path %>
